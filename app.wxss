@import './style/iconfont.wxss';
page,view,text,image,swiper,swiper-item,navigator{
  padding: 0;
  margin: 0;
  box-sizing: border-box;
}
/* 定义wxss全局变量 */
page{
  --themeColor: #f8554d;
  font-size: 28rpx;
}
/* 隐藏滚动条 */
::-webkit-scrollbar {
  display:none;
  width:0;
  height:0;
  color:transparent;
}
